  "author": "",
  "license": "ISC",
  "dependencies": {
    "fastify": "^4.25.2",
    "fastify-plugin": "^4.5.1",
    "poolifier": "^3.1.12"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "autocannon": "^7.14.0",
    "typescript": "^5.3.3"
  }
}
