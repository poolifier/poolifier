  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.19.2",
    "poolifier": "^4.0.13"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.13.0",
    "autocannon": "^7.15.0",
    "typescript": "^5.4.5"
  }
}
